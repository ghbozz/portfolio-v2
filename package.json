{
  "name": "portfolio-v2",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "push": "rm -rf dist && nuxt generate && yarn deploy",
    "cleanup": "git add . && git commit -m 'cleanup' && git push origin master && yarn push"
  },
  "dependencies": {
    "@nuxt/content": "^1.14.0",
    "@nuxtjs/axios": "^5.12.0",
    "@nuxtjs/style-resources": "^1.1.0",
    "@nuxtjs/tailwindcss": "^4.1.3",
    "nuxt": "^2.14.0",
    "push-dir": "^0.4.1"
  },
  "devDependencies": {
    "node-sass": "^6.0.0",
    "sass-loader": "^10.0.0"
  }
}
